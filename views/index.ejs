<html>
    <head>
        <title><%= title %></title>
    </head>
    <body>
        <% messages.forEach((message, index) => { %>
            <p><%= message.text %></p>
            <button class="details" onclick="window.location.href='/message/<%= index %>'">See Details</button>
        <% }) %>
        <a href="/new">Add Message</a>
    </body>
</html>